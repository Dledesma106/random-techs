{
    "build": {
        "development": {
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "https://ransys-test.vercel.app",
                "EXPO_PUBLIC_API_HOST": "https://ransys-test.vercel.app"
            }
        },
        "production": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "@env:EXPO_PUBLIC_API_BASE_URL",
                "EXPO_PUBLIC_API_HOST": "@env:EXPO_PUBLIC_API_HOST",
                "EXPO_PUBLIC_AWS_ACCESS_KEY_ID": "@env:EXPO_PUBLIC_AWS_ACCESS_KEY_ID",
                "EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY": "@env:EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY",
                "EXPO_PUBLIC_AWS_REGION": "@env:EXPO_PUBLIC_AWS_REGION",
                "EXPO_PUBLIC_AWS_BUCKET_NAME": "@env:EXPO_PUBLIC_AWS_BUCKET_NAME"
            }
        }
    }
}
